<h1>Add a Photo for <%= @kid.first_name %>'s <%= @milestone.title %></h1>

<%= form_tag "/kids/#{@kid.id}/milestones/#{@milestone.id}/images", multipart: true, method: :post do %>

  <div>
    <%= label_tag :avatar %>
    <%= file_field_tag :avatar %>
  </div>

  <%= submit_tag "Submit" %>

<% end %>